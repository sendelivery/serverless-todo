{
  "executionRoleArn": "TASK_EXEC_ROLE",
  "containerDefinitions": [
    {
      "essential": true,
      "image": "<IMAGE1_NAME>",
      "name": "APPLICATION",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "cpu": "256",
  "family": "fargate-task-definition",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"]
}
